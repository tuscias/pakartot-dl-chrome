{
  "manifest_version": 2,
  "name": "Pakartot.lt Downloader",
  "version": "0.1",
  "author": "Tuscias",
  "description": "Download music from https://www.pakartot.lt/ website",
  "content_scripts": [{
    "matches": ["https://www.pakartot.lt/album/*"],
    "js": ["lib/jquery-2.2.4.min.js", "lib/FileSaver.min.js", "lib/URI.js", "lib/jszip.min.js", "content.js"]
  }],
  "permissions": ["https://stream1.pakartot.lt/"],
  "icons": {
    "128": "images/icon.png"
  },
  "web_accessible_resources": [
       "images/spinner.gif",
       "images/icon.png",
       "images/download.png",
       "images/wait.png"
     ]
}